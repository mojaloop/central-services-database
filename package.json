    "faucet": "0.0.1",
    "istanbul": "1.1.0-alpha.1",
    "pre-commit": "1.2.2",
    "proxyquire": "2.1.0",
    "sinon": "7.2.4",
    "standard": "12.0.1",
    "tap-xunit": "2.3.0",
    "tape": "4.10.1",
    "tapes": "4.1.0"
  },
